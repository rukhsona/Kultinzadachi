// Вычисляет сопротивление электрической цепи 
float sopr( float rl, float г2, int t) 
( 
// rl,r2 - величины сопротивлений 
// t - тип соединения: 
// 1 - последовательное; 
// 2 - параллельное. 
// если тип соединения указан неверно, 
//то функция возвращает -1 
float г; 
if ( t==l) г = rl + г2; 
els e if (t== 2) г = г1*г2/(г1+г2); 
els e г = -1 ; 
return(г); 
}
