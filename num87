// Проверка знания истории архитектуры 
tinclude <stdio.h> 
tinclude <conio.h> 
void mainO 
{ 
int otv; // номер выбранного варианта ответа 
printf("Архитектор Исаакиевского собора:\п"); 
printf("1. Доменико Трезини\п"); 
printf("2. Огюст МонферранХп"); 
printf("3. Карл Росси\п"); 
printf("Введите номер ответа и нажмите <Enter>"); 
printf("-> ") ; 
scanf("%i", &otv); 
if (otv == 2) 
printf("Правильно."); 
else { 
printf("Вы ошиблись A n Архитектор Исаакиевского ") ; 
printf("собора Огюст Монферран.\n"); 
} 
printf("\пДля завершения нажмите <Enter>"); 
getch(); 
}
