printf("После ввода каждого числа ") ; 
printf("нажимайте <Enter>\n");
for (i = 0; i < SIZE; i++) 
{ 
printf("a[%i] ->",i+l); 
scanf("%i", &a[i]); 
if (a[i] != 0) n++; 
} 
printf("B массиве %i ненулевых элемента.\n", n); 
